<% if @drone.gallerys.attached? %>
<div class="row">
  <% @drone.gallerys.each do |pic| %>
  <div class="col-4">
      <%= link_to url_for(pic), 'data-dimbox': "photos" do %>
          <% image_tag url_for(pic), class: "gallery_img rounded-0" %>
      <% end %>
       </div>
  <% end %>
</div>
<% end %>