<% unless @component.errors.present? %>
  $('#component_<%= @component.id %>').remove();
  $('#components').html('<%= j render 'component' %>');
  $("#form_components").html("<%= j render 'components/form' %>");
<% else %>
  $('.component-errors-update').html('<%= j render 'shared/errors', resource: @component %>')
<% end %>
